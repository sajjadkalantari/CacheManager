{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CacheOptions": [
    {
      "Level": 0,
      "AbsoluteExpirationDay": 1,
      "AbsoluteExpirationRelativeToNowInMinutes": 15
    },
    {
      "Level": 1,
      "AbsoluteExpirationDay": 7,
      "AbsoluteExpirationRelativeToNowInMinutes": 60
    }
  ],
  "ApiEndPoints": {
    "BookEndPoint": "http://get.taaghche.ir/v2/book/{id}",
    "CommentEndPoint": "https://get.taaghche.ir/v2/comment/{id}/replies"
  }
  
}
